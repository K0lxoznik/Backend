{
	"name": "Backend",
	"version": "1.0.0",
	"main": "index.js",
	"packageManager": "yarn@4.1.0",
	"os": [
		"linux",
		"darwin",
		"win32"
	],
	"cpu": [
		"x64",
		"ia32",
		"arm64"
	],
	"scripts": {
		"ts": "tsc -w",
		"dev": "cross-env NODE_ENV=development nodemon dist/index.js",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"build": "tsc -p tsconfig.json",
		"migration:up": "npx typeorm migration:run -d typeormconfig.js",
		"typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/db/index.ts"
	},
	"repository": "https://github.com/K0lxoznik/Backend",
	"author": "Dima Panasenko <113297124+K0lxoznik@users.noreply.github.com>",
	"license": "MIT",
	"type": "commonjs",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"cookie": "^0.6.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.4",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"geoip-lite": "^1.4.9",
		"ioredis": "^5.3.2",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"nodemailer": "^6.9.9",
		"pg": "^8.11.3",
		"reflect-metadata": "^0.2.1",
		"sharp": "^0.33.2",
		"typeorm": "^0.3.20"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cookie": "^0.6.0",
		"@types/cookie-parser": "^1.4.6",
		"@types/cors": "^2.8.17",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.21",
		"@types/express-validator": "^3.0.0",
		"@types/geoip-lite": "^1.4.4",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.17",
		"@types/nodemailer": "^6.4.14",
		"@types/pg": "^8.11.0",
		"nodemon": "^3.0.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
